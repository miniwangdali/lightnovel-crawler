{
  "name": "lightnovel-crawler",
  "version": "0.0.1",
  "main": "/build/server/main.js",
  "directories": {},
  "scripts": {
    "start": "electron ./build/server/main.js",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "webpack --config ./webpack/webpack.dev.config.js",
    "dev": "nodemon --watch ./src/server",
    "dev:all": "cross-env NODE_ENV=development FORCE_COLOR=1 npm-run-all -l -p dev:client dev:server",
    "dev:client": "npm run build:client",
    "dev:server": "npm run build:server && electron ./build/server/main.js"
  },
  "author": "miniwangdali@gmail.com",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "core-js": "^3.0.1",
    "epub-gen": "^0.1.0",
    "express": "^4.16.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.7",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/react-router-dom": "^4.3.1",
    "autoprefixer": "^9.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^4.1.5",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "nodemonConfig": {
    "ext": "ts, tsx, js, jsx, scss, css, html, json",
    "ignore": [
      "test/*",
      "docs/*",
      "dist/*",
      "build",
      "webpack",
      "node_modules/*"
    ],
    "delay": "1000",
    "exec": "npm run dev:all"
  }
}
